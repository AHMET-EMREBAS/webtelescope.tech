{
  "name": "class-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/class-generator/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/class-generator",
        "main": "libs/class-generator/src/index.ts",
        "tsConfig": "libs/class-generator/tsconfig.lib.json",
        "assets": ["libs/class-generator/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs class-generator {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/class-generator/**/*.ts",
          "libs/class-generator/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/class-generator/jest.config.ts"
      }
    }
  },
  "tags": []
}
