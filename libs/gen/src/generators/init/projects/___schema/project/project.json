{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Project Schema",
    "properties": {
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "models": {
            "patternProperties": {
                "[a-zA-Z-]{3,}": {
                    "$ref": "./../model/model.json"
                }
            },
            "propertyNames": {
                "$ref": "./../enums/entity.json"
            }
        }
    },
    "required": [
        "models"
    ]
}