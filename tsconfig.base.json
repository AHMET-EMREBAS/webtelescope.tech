{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@webpackages/auth": ["libs/auth/src/index.ts"],
      "@webpackages/browser": ["libs/browser/src/index.ts"],
      "@webpackages/client-common": ["libs/client-common/src/index.ts"],
      "@webpackages/common": ["libs/common/src/index.ts"],
      "@webpackages/config": ["libs/config/src/index.ts"],
      "@webpackages/core": ["libs/core/src/index.ts"],
      "@webpackages/db": ["libs/db/src/index.ts"],
      "@webpackages/gen": ["libs/gen/src/index.ts"],
      "@webpackages/gen-client": ["libs/gen-client/src/index.ts"],
      "@webpackages/gen-crud": ["libs/gen-crud/src/index.ts"],
      "@webpackages/gen-entity": ["libs/gen-entity/src/index.ts"],
      "@webpackages/gen-form": ["libs/gen-form/src/index.ts"],
      "@webpackages/gen-meta": ["libs/gen-meta/src/index.ts"],
      "@webpackages/gen-model": ["libs/gen-model/src/index.ts"],
      "@webpackages/gen-rest": ["libs/gen-rest/src/index.ts"],
      "@webpackages/material": ["libs/material/src/index.ts"],
      "@webpackages/material/chart": ["libs/material/chart/src/index.ts"],
      "@webpackages/material/editor": ["libs/material/editor/src/index.ts"],
      "@webpackages/material/forms": ["libs/material/forms/src/index.ts"],
      "@webpackages/material/layout": ["libs/material/layout/src/index.ts"],
      "@webpackages/material/list": ["libs/material/list/src/index.ts"],
      "@webpackages/material/services": ["libs/material/services/src/index.ts"],
      "@webpackages/material/table": ["libs/material/table/src/index.ts"],
      "@webpackages/meta": ["libs/meta/src/index.ts"],
      "@webpackages/meta-printer": ["libs/meta-printer/src/index.ts"],
      "@webpackages/printer": ["libs/printer/src/index.ts"],
      "@webpackages/utils": ["libs/utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp", "docs", "assets", "tasks"]
}
